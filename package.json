{
  "name": "eleventy-personal-site",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "eleventy --watch --quiet & parcel watch './_tmp/**/*.html' --dist-dir ./_site --no-source-maps --no-cache & sirv ./_site --dev --host",
    "build": "eleventy && parcel build './_tmp/**/*.html' --dist-dir ./_site --no-source-maps"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.1.0",
    "@tailwindcss/typography": "^0.5.4",
    "eleventy-plugin-time-to-read": "^1.2.0",
    "luxon": "^3.0.1",
    "parcel": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.12",
    "sirv-cli": "^2.0.2",
    "tailwindcss": "^3.1.6"
  },
  "browserslist": "defaults",
  "dependencies": {
    "gsap": "^3.10.4"
  }
}
